{
    "name" : "the_it_nerd/module-social-login",
    "type" : "magento2-module",
    "license" : "OSL-3.0",
    "authors" : [
        {
            "name" : "Raul Sakai",
            "email" : "raul.sakai@theitnerd.io"
        }
    ],
    "description" : "The IT Nerd - Magento 2 Social Login Module.",
    "homepage" : "https://github.com/the-it-nerd/magento-module-social-login",
    "keywords" : [
        "magento",
        "magento2"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require" : {
        "magento/framework" : ">=102.0.0",
        "magento/magento-composer-installer" : "*",
        "the_it_nerd/module-core" : "*",
        "facebook/graph-sdk": "*",
        "google/apiclient": "^2.15",
        "league/oauth2-facebook": "^2.0"
    },
    "autoload" : {
        "files" : [
            "registration.php"
        ],
        "psr-4" : {
            "TheITNerd\\SocialLogin\\" : ""
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true,
            "magento/magento-composer-installer": true
        }
    }
}
